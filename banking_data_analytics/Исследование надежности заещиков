   "source": [
    "def total_income_categorizer(total_income):\n",
    "    if total_income <= 100000:\n",
    "        return 'Доход низкий'\n",
    "    if total_income >= 100000 and total_income <=200000:\n",
    "        return 'Доход средний'    \n",
    "    else:\n",
    "        return 'Доход высокий'\n",
    " \n",
    "df['total_income_category']= df['total_income'].apply(total_income_categorizer)        \n",
    "df.head(10)"
   ]
