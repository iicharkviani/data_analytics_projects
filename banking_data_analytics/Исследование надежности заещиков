   "source": [
    "df.groupby('income_type').agg({'total_income':['median']}).reset_index()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "median_incomes = df.groupby('income_type')['total_income'].median().to_dict()\n",
    " \n",
    "def filler_func(row):\n",
    "    if pd.isna(row['total_income']):\n",
    "        return median_incomes[row['income_type']]\n",
    "    return row['total_income']\n",
    " \n",
    "df['total_income'] = df.apply(filler_func, axis=1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
