       "8          свадьба  \n",
       "9     недвижимость  "
      ]
     },
     "execution_count": 14,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def purpose_categorizer(purpose):\n",
    "    purpose = m.lemmatize(purpose)\n",
    "    purpose = \" \".join(purpose)\n",
    "    if 'автомобиль' in purpose:\n",
    "        return 'автомобиль'\n",
    "    if 'образование' in purpose:\n",
    "        return 'образование'\n",
    "    if 'недвижимость' in purpose or 'жилье' in purpose:\n",
    "        return 'недвижимость'\n",
    "    else:\n",
    "        return 'свадьба'\n",
    " \n",
    "df['purpose_category'] = df['purpose'].apply(purpose_categorizer)       \n",
    "df.head(10)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Посмотрим какая цель получения кредита наиболее распространена "
   ]
  },
